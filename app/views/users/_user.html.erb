<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Surname:</strong>
    <%= user.surname %>
  </p>

  <p>
    <strong>Birthdate:</strong>
    <%= user.birthdate %>
  </p>

  <p>
  <strong>Email:</strong>
  <%= user.email %>
</p>

<p>
  <strong>DNI:</strong>
  <%= user.dni %>
</p>

<p>
  <strong>Celular:</strong>
  <%= user.phone %>
</p>

</div>
