<div id="<%= dom_id user %>">
  <p>
    <strong>Nombre:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Apellido:</strong>
    <%= user.surname %>
  </p>

  <p>
  <strong>DNI:</strong>
  <%= user.dni %>
  </p>

  <p>
  <strong>Celular:</strong>
  <%= user.phone %>
  </p>

  <p>
    <strong>Fecha de Nacimiento:</strong>
    <%= user.birthdate %>
  </p>

  <p>
  <strong>Email:</strong>
  <%= user.email %>
  </p>

</div>
