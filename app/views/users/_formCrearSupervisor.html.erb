<%= simple_form_for(@user, url: users_create_su_url) do |f| %>
  <%= f.input :name, required: true, autofocus: true, input_html: { autocomplete: "name" } %>
  <%= f.input :surname, required: true, autofocus: true, input_html: { autocomplete: "surname" } %>
  <%= f.input :birthdate, required: true, autofocus: true, input_html: { autocomplete: "birthdate" } %>
  <%= f.input :phone, required: true, autofocus: true, input_html: { autocomplete: "birthdate" } %>
  <%= f.input :dni, required: true, autofocus: true, input_html: { autocomplete: "dni" } %>
  <%= f.input :email, required: true, autofocus: true, input_html: { autocomplete: "email" } %>
  <%= f.input :password, required: true, autofocus: true, input_html: { autocomplete: "email" } %>
  <%= f.button :submit %>
<% end %>